{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "compose-up",
            "command": "docker",
            "args": [
                "compose",
                "-f",
                "docker-compose.dev.yml",
                "up",
                "--build"
            ],
            "type": "shell"
        },
        {
            "label": "compose-down",
            "command": "docker",
            "args": ["compose", "-f", "docker-compose.dev.yml", "down"],
            "type": "shell"
        }
    ]
}
